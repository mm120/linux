&ifc_nand {
	partition@0000000 { reg = <0x0000000 0x1000000>; label = "NAND Persistent File System"; };
	partition@1000000 { reg = <0x1000000 0xf000000>; label = "NAND Upgrade Section"; };

	partition@olk_1000000 { reg = <0x1000000 0x0C00000>; label = "NAND P1 Boot File System"; };
	partition@olk_1800000 { reg = <0x1C00000 0x1C00000>; label = "NAND P1 Read-Write File System"; };
	partition@olk_3800000 { reg = <0x3800000 0x5000000>; label = "NAND P1 Root File System"; };
	partition@olk_8800000 { reg = <0x8800000 0x0C00000>; label = "NAND P2 Boot File System"; };
	partition@olk_9000000 { reg = <0x9400000 0x1C00000>; label = "NAND P2 Read-Write File System"; };
	partition@olk_B000000 { reg = <0xb000000 0x5000000>; label = "NAND P2 Root File System"; };
};
